class Solution {
    public boolean isPalindrome(int x) {
       
        if(x<0){
            return(false);
        }  
        int a=x;
        int b=0;
        while(a>0){
            int r=a%10;
            b=b*10+r;
            a-=r;
            a/=10;
        }
        if(b==x){
            return(true);
        }
        return(false);
    }
}
